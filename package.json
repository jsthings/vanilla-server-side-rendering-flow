{
  "name": "dla_data_grid",
  "version": "0.0.1",
  "description": "React - React Router - Redux | Express | Webpack -> Vanilla Server Side Rendering Full Work Flow",
  "main": "server.development.js",
  "scripts": {
    "start": "npm run dev-dll",
    "test": "./test/test.js",
    "clean": "node ./node_modules/rimraf/bin.js ./dist",
    "clean-dll": "node ./node_modules/rimraf/bin.js ./lib/*.*",
    "env-development": "set NODE_ENV=development",
    "env-production": "set NODE_ENV=production",
    "dll": "npm run clean-dll && echo 'Building DLLs ...' && node ./node_modules/@babel/node/bin/babel-node.js ./node_modules/webpack/bin/webpack.js --config ./webpack.config.dll.js",
    "dev-mon": "NODE_PATH=./:./src:./src/js ./node_modules/nodemon/bin/nodemon.js --config ./nodemon.json --exec ./node_modules/@babel/node/bin/babel-node.js ./server/server.development.js",
    "dev-mon-dll": "NODE_PATH=./:./src:./src/js ./node_modules/nodemon/bin/nodemon.js --config ./nodemon.json --exec ./node_modules/@babel/node/bin/babel-node.js ./server/server.development.js --dll",
    "dev-mon-win": "NODE_PATH=./;./src;./src/js ./node_modules/nodemon/bin/nodemon.js --config ./nodemon.json --exec ./node_modules/@babel/node/bin/babel-node.js ./server/server.development.js",
    "dev": "npm run clean && echo 'Running Development ...' && npm run env-development && npm run dev-mon",
    "dev-dll": "npm run clean && echo 'Running Development ...' && npm run env-development && npm run dev-mon-dll",
    "dev-win": "npm run clean && echo 'Running Development ...' && npm run env-development && npm run dev-mon-win",
    "prod": "node ./node_modules/@babel/cli/bin/babel.js ./server/server.production.js -o ./server/server.production.legacy.js && NODE_PATH=./:./src:./src/js node ./server/server.production.legacy.js",
    "prod-win": "node ./node_modules/@babel/cli/bin/babel.js ./server/server.production.js -o ./server/server.production.legacy.js && NODE_PATH=./;./src;./src/js node ./server/server.production.legacy.js",
    "build": "npm run clean && echo 'Running Production ...' && npm run env-production && npm run prod",
    "build-win": "npm run clean && echo 'Running Production ...' && npm run env-production && npm run prod-win"
  },
  "keywords": [
    "React",
    "Router",
    "Redux",
    "Express",
    "Webpack",
    "Vanilla",
    "Server Side Rendering",
    "Full",
    "Work",
    "Flow"
  ],
  "author": "UP",
  "license": "ISC",
  "dependencies": {},
