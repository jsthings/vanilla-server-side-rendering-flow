{
  "presets": [
    [
      "@babel/env",
      {
        "loose": true,
        "targets": {
          "browsers": [
            "last 2 versions",
            "ie >= 10",
            "safari >= 7"
          ]
        },
        "include": [
          "transform-classes"
        ]
      }
    ],
    "@babel/react"
  ],
  "env": {
    "development": {
      "plugins": [
        [
          "react-hot-loader/babel"
        ],
        [
          "direct-import",
          {
            "modules": [
              "@material-ui/core",
              "@material-ui/icons"
            ]
          }
        ],
        [
          "@babel/proposal-class-properties"
        ],
        [
          "@babel/proposal-decorators", { "legacy": true }
        ]
      ]
    },
    "production": {
      "plugins": [
        [
          "direct-import",
          {
            "modules": [
              "@material-ui/core",
              "@material-ui/icons"
            ]
          }
        ],
        [
          "@babel/proposal-class-properties"
        ],
        [
          "@babel/proposal-decorators", { "legacy": true }
        ]
      ]
    }
  }
}